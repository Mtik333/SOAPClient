<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controllers.ChangeReservation">
    <children>
        <VBox prefHeight="200.0" prefWidth="242.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="235.0">
                    <children>
                        <Label text="Select seat"/>
                        <ChoiceBox fx:id="choiceBox" prefWidth="150.0"/>
                    </children>
                </HBox>
                <Button fx:id="changeButton" mnemonicParsing="false" text="Edit"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
